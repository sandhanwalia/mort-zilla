{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "public",
    "framework": "vite",
    "routes": [
        {
            "src": "/assets/(.*)",
            "headers": { "cache-control": "public, max-age=31536000, immutable" },
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
} 